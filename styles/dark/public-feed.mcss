PublicFeed {
  margin: auto

  section {
    padding: 20px 20px 20px 20px
    width: 700px

    -notFollowingAnyoneWarning {
      border: 1px solid #ffba4e
      background: #5f410b
      margin: 5px 0
      color: #ffdda7
      font-size: 110%
    }

    h1 {
      font-size: 120%
      font-weight: bold
      margin: 0
      color: white
    }

    p {
      margin: 0
      margin-top: 8px
    }
  }
}
